{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}

    <h1>All Users</h1>
    <ul>
        {% for user in users %}
        <li><a href="{{ path('profile', {'id': user.id}) }}">{{ user.firstname }} | {{ user.lastname }} | {{ user.email }}</li>
        {% endfor %}
    </ul>
<div class="example-wrapper">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
</div>
{% endblock %}
